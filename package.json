{
  "name": "base-bot",
  "version": "1.0.0",
  "description": "A basic template bot using discord.js including folder based event / command routing for ease of expandability and deployment.",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy:dev": "node deploy/deploy_commands.js dev",
    "deploy:prod": "node deploy/deploy_commands.js prod",
    "deploy:global": "node deploy/deploy_commands.js global",
    "delete:dev": "node deploy/delete_command.js dev",
    "delete:prod": "node deploy/delete_command.js prod",
    "delete:global": "node deploy/delete_command.js global"
  },
  "repository": {
    "type": "git",
