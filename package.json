  },
  "homepage": "https://github.com/Aldoraz/base-bot#readme",
  "dependencies": {
    "colorette": "^2.0.20",
    "discord.js": "^14.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "eslint": "^9.13.0",
    "globals": "^15.11.0"
  }
